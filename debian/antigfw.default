logfile  = '/var/log/antigfw.log'
loglevel = 'WARNING'
daemon   = False
pidfile  = '/var/run/antigfw.pid'
servers  = [
    {'proxyport':'7777', 'username':'user', 'sshhost':'sshhost',
     'sshport':'22', 'sshprivfile':'~/.ssh/id_rsa'},
    ]
uniproxy = True

max_conn  = 30
socks     = None
filter    = ['/etc/uniproxy/gfw', '/usr/share/uniproxy/gfw']
localip   = ''
localport = 8118
